{
  "name": "@nation3labs/promptella-sdk",
  "version": "1.0.6",
  "description": "Official TypeScript SDK for Promptella API",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run type-check && npm run build"
  },
  "keywords": [
    "ai",
    "prompt-engineering",
    "typescript",
    "sdk",
    "promptella"
  ],
  "author": "Promptella Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nat3Labs/promptella-sdk.git"
  },
  "peerDependencies": {
    "typescript": ">=4.5.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
